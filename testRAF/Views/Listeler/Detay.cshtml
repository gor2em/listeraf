@model testRAF.ViewModels.myViewModel
@{
    ViewBag.Title = "Listeraf / Listeler";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@foreach (var item in Model.listeler)
{
<div class="container-full">

    <div class="col-lg-12  text-center">
        <img src="@item.listeResim.ListeOrtaYol" class="img-border-level-liste img-circle" width="200" height="200" />
    </div>

    <div class="col-lg-12 text-center">
        <h1>@item.ListeBaslik</h1>
        <h5>@item.ListeAciklama</h5>
    </div>

    <div>@Html.Raw(item.ListeIcerik)</div>
    <p>Ekleyen: @item.User.UserName</p>
    <p>Eklenme Tarihi: @item.ListeTarih.Value.ToShortDateString()</p>
    <p>beğenme @item.begenme</p>
    <p>görüntülenme @item.goruntulenme</p>
</div>
}

